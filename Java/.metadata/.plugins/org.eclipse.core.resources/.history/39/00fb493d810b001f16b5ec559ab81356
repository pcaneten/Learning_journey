package bruchtest;

public class Bruch {
	int zaehler;
	int nenner;
	
	void ausgeben() {
		System.out.println(zaehler + "/" + nenner);
	}
	
	void ausgabeLang() {
		System.out.println("Der Zeahler des Bruchs ist hat den Wert: " + zaehler);
		System.out.println("Der Nenner des Bruchs ist hat den Wert: " + nenner);
		System.out.println(zaehler + "/" + nenner);
	}


}

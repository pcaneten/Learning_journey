package bruchtest;

public class Bruchtest1 {

	public static void main(String[] args) {
		Bruch bruch1 = new Bruch();
		bruch1.nenner = 8;
		System.out.println(bruch1.nenner);
		
		Bruch lieblingsBruch = new Bruch();
		lieblingsBruch.nenner = 25;
		lieblingsBruch.zaehler = 75;
		System.out.println(lieblingsBruch.nenner + "/" + lieblingsBruch.zaehler);
		
		bruch1.ausgeben2();
		lieblingsBruch.ausgeben2();
		bruch1.ausgabeDezimalBruch();
		lieblingsBruch.ausgabeDezimalBruch();
	}
}

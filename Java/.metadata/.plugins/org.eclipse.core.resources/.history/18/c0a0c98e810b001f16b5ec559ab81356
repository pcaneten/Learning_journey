package bruchtest;

public class Bruchtest1 {

	public static void main(String[] args) {
		Bruch a = new Bruch();
		a.zaehler = 3;
		a.nenner = 4;
		a.ausgeben();
		a.kuerzen();
		a.ausgeben();
		
		Bruch b = new Bruch();
		b.zaehler = 50;
		b.nenner = 150;
		b.ausgabeLang();
		b.kuerzen();
		b.ausgabeLang();
		

	}
}
